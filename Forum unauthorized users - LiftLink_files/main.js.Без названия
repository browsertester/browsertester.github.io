    function redirect(page) {
        const link = window.location.origin + page;
        window.location.href = link;
      }

      function copyLink(link) {
        // Копируем ссылку в буфер обмена
        navigator.clipboard.writeText(link).then(() => {
          // Выводим сообщение об успешном копировании
          alert('Ссылка скопирована!');
        }).catch(err => {
          console.error('Ошибка при копировании ссылки:', err);
        });
      }

      function copyReferalLink(link) {
        // Копируем ссылку в буфер обмена
        navigator.clipboard.writeText(link).then(() => {
          // Выводим сообщение об успешном копировании
          alert('Ваша реферальная ссылка была скопирована!');
        }).catch(err => {
          console.error('Ошибка при копировании ссылки:', err);
        });
      }
      
    

      const usernameUsername = currentUsername; // Замените на реальное имя пользователя
      const inactiveTimeLimit = 60 * 60 * 1000; // 30 минут в миллисекундах
    
      function updateLastUpdate() {
        const currentTime = Date.now();
        localStorage.setItem('lastActivityTime', currentTime);
        
        // Отправка запроса на сервер для обновления lastUpdate
        fetch('/updateLastUpdate', {
          method: 'POST',
          headers: {
            'Content-Type': 'application/json'
          },
          body: JSON.stringify({ username: usernameUsername })
        }).then(response => {
          if (!response.ok) {
            console.error('Failed to update lastUpdate on server');
          }
        }).catch(error => {
          console.error('Failed to update lastUpdate:', error);
        });
      }
    
      function checkInactivity() {
        const lastActivityTime = localStorage.getItem('lastActivityTime');
        const currentTime = Date.now();
    
        if (lastActivityTime && (currentTime - lastActivityTime) > inactiveTimeLimit) {
          window.location.href = '/timenow';
        } else {
          setTimeout(checkInactivity, 10000); // Проверяем каждые 1 секунду
        }
      }
    
      function debounce(func, wait) {
        let timeout;
        return function(...args) {
          clearTimeout(timeout);
          timeout = setTimeout(() => func.apply(this, args), wait);
        };
      }
    
      const debouncedUpdateLastUpdate = debounce(updateLastUpdate, 5000); // Дебаунсинг с задержкой в 5 секунд

    
      function resetTimer() {
        debouncedUpdateLastUpdate();
      }
    
      document.addEventListener('mousemove', resetTimer);
      document.addEventListener('keypress', resetTimer);
      document.addEventListener('click', resetTimer);
      document.addEventListener('scroll', resetTimer);
    
      // Инициализируем проверку при загрузке страницы
      updateLastUpdate();
      checkInactivity();


          const searchInput = document.getElementById('search-users');
          const userList = document.getElementById('searched-users');
          const users = document.querySelectorAll('.list > li');
      
          searchInput.addEventListener('input', function() {
              const searchTerm = this.value.trim().toLowerCase();
              const minCharsToShow = 2;
      
              if (searchTerm.length < minCharsToShow) {
                  // Удаляем лишних пользователей из DOM
                  for (let i = 30; i < users.length; i++) {
                      userList.removeChild(users[i]);
                  }
              } else {
                  // Показываем всех пользователей, если необходимо
                  // Проверка на добавление новых пользователей не требуется, так как они не были загружены в DOM
              }
          });


