              },
            "или": {
                "en": "or",
                "es": "o"
            },
            
            'пол': {
                'en': 'Gender',
                'es': 'Género'
            },
            'Ваш': {
                'en': 'Your',
                'es': 'Su'
            },
            'вы': {
                'en': 'you',
                'es': 'vosotros'
            },
            "в": {
                "en": "in",
                "es": "en"
            },
            "и": {
                "en": "and",
                "es": "e"
            }     
        };
        
            
            function translateText(node) {
                if (node.nodeType === Node.TEXT_NODE) {
                    let text = node.nodeValue.trim().toLowerCase();
                    let lastChar = text.charAt(text.length - 1);
                    if (lastChar === '.' || lastChar === ':' || !isNaN(lastChar) || lastChar === '(' || lastChar === ')') {
                        text = text.slice(0, -1);
                    }
                    for (let key in translationDict) {
                        if (text.includes(key.toLowerCase())) {
                            let translatedText = translationDict[key] && translationDict[key][language];
                            if (translatedText) {
                                node.nodeValue = node.nodeValue.replace(new RegExp(key, 'gi'), translatedText);
                            }
                        }
                    }
                } else if (node.nodeType === Node.ELEMENT_NODE) {
                    if (!node.classList.contains('scrapbook-content') && !node.classList.contains('scrapbook-detail') && !node.classList.contains('eventing-title') && !node.classList.contains('no-translate')) {
                        node.childNodes.forEach(translateText);
                        if (node.attributes) {
                            Array.from(node.attributes).forEach(attr => {
                                let attrValue = node.getAttribute(attr.name);
                                if (attrValue) {
                                    let translatedAttrValue = translationDict[attrValue.toLowerCase()] ? translationDict[attrValue.toLowerCase()][language] : null;
                                    if (translatedAttrValue) {
                                        node.setAttribute(attr.name, translatedAttrValue);
                                    }
                                }
                            });
                        }
                        // Перевод placeholder для input
                        if (node.nodeName.toLowerCase() === 'input' && node.getAttribute('placeholder')) {
                            let placeholderText = node.getAttribute('placeholder').trim().toLowerCase();
                            for (let key in translationDict) {
                                if (placeholderText.includes(key.toLowerCase())) {
                                    let translatedPlaceholderText = translationDict[key] && translationDict[key][language];
                                    if (translatedPlaceholderText) {
                                        node.setAttribute('placeholder', translatedPlaceholderText);
                                        break;
                                    }
                                }
                            }
                        }
                        // Перевод placeholder для textarea
                        if (node.nodeName.toLowerCase() === 'textarea' && node.getAttribute('placeholder')) {
                            let placeholderText = node.getAttribute('placeholder').trim().toLowerCase();
                            for (let key in translationDict) {
                                if (placeholderText.includes(key.toLowerCase())) {
                                    let translatedPlaceholderText = translationDict[key] && translationDict[key][language];
                                    if (translatedPlaceholderText) {
                                        node.setAttribute('placeholder', translatedPlaceholderText);
                                        break;
                                    }
                                }
                            }
                        }
                    }
                }
            }
            
        
        // Начинаем переводить текст с корневого элемента body
        translateText(document.body);
          }
        
        // Вызов функции перевода после полной загрузки DOM
        
            

        
          